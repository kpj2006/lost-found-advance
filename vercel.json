{
  "version": 2,
  "builds": [
    {
      "src": "server/vercel.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["server/**", "shared/**", "dist/public/**"],
        "runtime": "nodejs18.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/vercel.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
